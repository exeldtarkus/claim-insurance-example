// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x", "debian-openssl-3.0.x", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./NextDatabase.db"
}

model slik_progress_bar {
  id                  Int     @id @default(autoincrement())
  request_id          String
  period              String
  schema              Schema
  progress_bar_percent Int
  status              Status
  message             String
}

enum Schema {
  D01
  F01
}

enum Status {
  done
  failed
  on_progress
  complete
}
